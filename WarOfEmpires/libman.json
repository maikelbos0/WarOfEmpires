{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.0.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "scss/**/*.*",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.js.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.3",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@vdt-jquery/jquery-rangeslider@1.0.1",
      "destination": "wwwroot/lib/jquery-rangeslider/",
      "files": [
        "Content/jquery-rangeslider.css",
        "Content/jquery-rangeslider.min.css",
        "Content/jquery-rangeslider.style.min.css",
        "Content/jquery-rangeslider.style.css",
        "Scripts/jquery-rangeslider.js",
        "Scripts/jquery-rangeslider.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@vdt-jquery/jquery-datagridview@1.3.1",
      "destination": "wwwroot/lib/jquery-datagridview/",
      "files": [
        "Content/jquery-datagridview.css",
        "Content/jquery-datagridview.min.css",
        "Content/jquery-datagridview.style.css",
        "Content/jquery-datagridview.style.min.css",
        "Scripts/jquery-datagridview.js",
        "Scripts/jquery-datagridview.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "toastr@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.js",
        "build/toastr.min.js",
        "build/toastr.min.css",
        "build/toastr.css"
      ]
    }
  ]
}