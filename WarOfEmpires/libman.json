{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.0.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "scss/**/*.*"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.js"
      ]
    }
  ]
}